{
  "name": "playwright-e2e-tests",
  "version": "1.0.0",
  "description": "Playwright E2E Tests with AI Capabilities include MCP",
  "keywords": [
    "playwright",
    "playwright-e2e-tests",
    "playwright-mcp"
  ],
  "license": "ISC",
  "author": "Minh Vo Automation",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test:api": "npx playwright test tests/api/users.api.spec.ts --config=config/test.playwright.config.ts ",
    "test:e2e": "npx playwright test tests/e2e/nopcommerce.e2e.spec.ts --config=config/test.playwright.config.ts --headed",
    "test:make-apt": "npx playwright test tests/functional/make.aptmnt.spec.ts --config=config/test.playwright.config.ts --headed",
    "dev:make-apt": "npx playwright test tests/functional/make.aptmnt.spec.ts --config=config/dev.playwright.config.ts --headed",
    "demo:params": "npx playwright test tests/demo/params.make.aptmnt.spec.ts --headed",
    "demo:parellel": "npx playwright test --grep @demo",
    "demo:tag": "npx playwright test --grep @smoke --headed",
    "demo:hooks": "npx playwright test tests/demo/hooks-demo.spec.ts --headed",
    "make-apt": "npx playwright test tests/functional/make.aptmnt.spec.ts --headed",
    "login": "npx playwright test tests/functional/login.spec.ts --headed",
    "demo": "npx playwright test tests/demo/mytest.spec.ts --headed"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/node": "^25.0.10",
    "allure-playwright": "^3.4.5",
    "chalk": "^5.6.2",
    "dotenv": "^17.3.1"
  }
}
